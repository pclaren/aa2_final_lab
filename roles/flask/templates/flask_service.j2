# {{ ansible_managed }}

[Unit]
Description=Launch flask app {{ flask_app_name }}
After=network.target

[Service]
User={{ flask_user }}
Group={{ flask_group }}
WorkingDirectory={{ flask_home }}/{{ flask_app_name }}
ExecStart={{ flask_app_lauch_path }}/launch_{{ flask_app_name }}

[Install]
WantedBy=multi-user.target
